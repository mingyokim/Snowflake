<!-- A .test file which launches the node and its corresponding rostest -->

<launch>
    <node name="line_extractor_node" pkg="line_extractor_igvc" type="line_extractor_node">
        <param name="degree_polynomial" value="3" type="int" />
        <param name="lambda" value="0" type="double" />
        <param name="min_neighbours" value="1" type="int" />
        <param name="radius" value="80" type="double" />
    </node>
    <test test-name="line_extractor_rostest" pkg="line_extractor_igvc" type="line_extractor_rostest" />
</launch>
